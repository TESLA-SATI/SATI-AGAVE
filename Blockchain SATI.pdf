import requests
from PyPDF2 import PdfReader
from io import BytesIO

url = "https://github.com/TESLA-SATI/Blockchain-SATI/raw/SATI/33/Blockchain%20SATI.pdf"
response = requests.get(url)

try:
    reader = PdfReader(BytesIO(response.content))
    print("Valid PDF. 11:", len(reader.pages))
except Exception as e:
    print("Invalid PDF or cannot be read:", e)
